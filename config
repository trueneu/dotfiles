AddKeysToAgent yes
UseKeychain yes
ForwardAgent yes
RemoteForward 60158 127.0.0.1:60158

ServerAliveInterval     60
TCPKeepAlive            yes
KeepAlive               yes
VerifyHostKeyDNS        yes
StrictHostKeyChecking   no
ForwardX11              no

# # Development using direct access.

Host *.dev
    ForwardAgent        yes
    User                pgurkov
    Hostname            pgurkov-%h.booking.com
    ProxyCommand        none

Host *.dev.booking.com
    ForwardAgent        yes
    User                pgurkov
    ProxyCommand        none

# Jump host and SSH Key Injector using direct access.

Host ssh.booking.com citadel-* *citadel*.booking.com shell-* *.shell.booking.com *.lom.booking.com
    ForwardAgent        yes
    User                pgurkov
    ProxyCommand        none

# Production using citadel.booking.com.

Host !gitlab.booking.com !*.dev.booking.com *-?? *-??? *-???? 10.* *.booking.com
    ForwardAgent        yes
    User                pgurkov
    ProxyCommand        ssh -q citadel.booking.com ssh-proxy %h %p

Host github.com
    IdentityFile ~/.ssh/id_rsa

# Include ~/.ssh/config.booking
